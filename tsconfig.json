{
	"include": ["src"],
	"compilerOptions": {
		"outDir": "dist/src",
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "ES6",
		"esModuleInterop": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"build": true,
		"incremental": true
	},
	"exclude": ["./node_modules"]
}
